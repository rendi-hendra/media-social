{
  "openapi": "3.0.3",
  "info": {
    "title": "Media Social API",
    "version": "1.0",
    "description": "OpenAPI for Media social RESTfull API",
    "contact": {
      "name": "Rendi Hendra Syahputra",
      "email": "rendihendra2004@gmail.com",
      "url": "https://github.com/rendi-hendra"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "servers": [
    {
      "description": "API Server",
      "url": "https://api.{environment}.server.test/v1",
      "variables": {
        "environment": {
          "description": "Server Environment",
          "default": "dev",
          "enum": ["dev", "qa", "prod"]
        }
      }
    }
  ],
  "paths": {
    "/login": {
      "post": {
        "tags": ["Auth"],
        "summary": "User Login",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "Login": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string"
                    },
                    "email": {
                      "type": "string"
                    },
                    "password": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
